{
  "name": "Robot Framework Material Light",
  "type": "light",
  "colors": {
    "editor.background": "#FAFAFA",
    "editor.foreground": "#263238",
    "activityBarBadge.background": "#FF6F00",
    "sideBarTitle.foreground": "#777777",
    "list.activeSelectionBackground": "#E7E7E7",
    "list.activeSelectionForeground": "#00897B",
    "list.inactiveSelectionBackground": "#E7E7E7",
    "list.hoverBackground": "#E7E7E720",
    "list.dropBackground": "#E7E7E7",
    "statusBar.background": "#FAFAFA",
    "statusBar.foreground": "#90A4AE",
    "statusBar.noFolderBackground": "#FAFAFA",
    "statusBar.debuggingBackground": "#FF5370",
    "statusBar.debuggingForeground": "#FFFFFF",
    "activityBar.background": "#FAFAFA",
    "activityBar.foreground": "#263238",
    "editorGroup.border": "#E7E7E7",
    "editorGroupHeader.tabsBackground": "#E7E7E7",
    "tab.activeBackground": "#FAFAFA",
    "tab.activeForeground": "#263238",
    "tab.inactiveBackground": "#E7E7E7",
    "tab.inactiveForeground": "#90A4AE",
    "tab.border": "#E7E7E7",
    "panel.background": "#FAFAFA",
    "panel.border": "#E7E7E7",
    "terminal.background": "#FAFAFA",
    "terminal.foreground": "#263238"
  },
  "tokenColors": [
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#90A4AE",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword.control.section.robot", "entity.name.section.robot"],
      "settings": {
        "foreground": "#7C4DFF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.function.settings.robot", "entity.name.function.setting.robot"],
      "settings": {
        "foreground": "#2196F3",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["variable.parameter.robot", "variable.other.robot"],
      "settings": {
        "foreground": "#FF6F00"
      }
    },
    {
      "scope": ["entity.name.function.keyword.robot", "entity.name.function.test.robot"],
      "settings": {
        "foreground": "#00897B",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword.control.flow.robot"],
      "settings": {
        "foreground": "#7C4DFF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["support.function.builtin.robot"],
      "settings": {
        "foreground": "#F76D47"
      }
    },
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#388E3C"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#F76D47"
      }
    }
  ]
}
