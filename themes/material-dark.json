{
  "name": "Robot Framework Material Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#263238",
    "editor.foreground": "#EEFFFF",
    "activityBarBadge.background": "#FF6F00",
    "sideBarTitle.foreground": "#BBBBBB",
    "list.activeSelectionBackground": "#1E272C",
    "list.activeSelectionForeground": "#80CBC4",
    "list.inactiveSelectionBackground": "#1E272C",
    "list.hoverBackground": "#1E272C20",
    "list.dropBackground": "#1E272C",
    "statusBar.background": "#263238",
    "statusBar.foreground": "#546E7A",
    "statusBar.noFolderBackground": "#263238",
    "statusBar.debuggingBackground": "#FF5370",
    "statusBar.debuggingForeground": "#FFFFFF",
    "activityBar.background": "#263238",
    "activityBar.foreground": "#FFFFFF",
    "editorGroup.border": "#1E272C",
    "editorGroupHeader.tabsBackground": "#1E272C",
    "tab.activeBackground": "#263238",
    "tab.activeForeground": "#FFFFFF",
    "tab.inactiveBackground": "#1E272C",
    "tab.inactiveForeground": "#546E7A",
    "tab.border": "#1E272C",
    "panel.background": "#263238",
    "panel.border": "#1E272C",
    "terminal.background": "#263238",
    "terminal.foreground": "#EEFFFF",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiRed": "#FF5370",
    "terminal.ansiGreen": "#C3E88D",
    "terminal.ansiYellow": "#FFCB6B",
    "terminal.ansiBlue": "#82AAFF",
    "terminal.ansiMagenta": "#C792EA",
    "terminal.ansiCyan": "#89DDFF",
    "terminal.ansiWhite": "#FFFFFF"
  },
  "tokenColors": [
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#546E7A",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword.control.section.robot", "entity.name.section.robot"],
      "settings": {
        "foreground": "#C792EA",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.function.settings.robot", "entity.name.function.setting.robot"],
      "settings": {
        "foreground": "#82AAFF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["variable.parameter.robot", "variable.other.robot"],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "scope": ["entity.name.function.keyword.robot", "entity.name.function.test.robot"],
      "settings": {
        "foreground": "#89DDFF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword.control.flow.robot"],
      "settings": {
        "foreground": "#C792EA",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["support.function.builtin.robot"],
      "settings": {
        "foreground": "#F78C6C"
      }
    },
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#F78C6C"
      }
    }
  ]
}
