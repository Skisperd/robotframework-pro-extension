{
  "Test Case": {
    "prefix": "test",
    "body": [
      "${1:Test Name}",
      "    [Documentation]    ${2:Test documentation}",
      "    [Tags]    ${3:tag1}",
      "    ${4:Log}    ${5:Test message}",
      "    $0"
    ],
    "description": "Create a new test case"
  },
  "Keyword": {
    "prefix": "keyword",
    "body": [
      "${1:Keyword Name}",
      "    [Documentation]    ${2:Keyword documentation}",
      "    [Arguments]    ${3:\\${arg1\\}}",
      "    ${4:Log}    ${5:\\${arg1\\}}",
      "    [Return]    ${6:\\${result\\}}",
      "    $0"
    ],
    "description": "Create a new keyword"
  },
  "For Loop": {
    "prefix": "for",
    "body": [
      "FOR    \\${${1:item}}    IN    @{${2:list}}",
      "    ${3:Log}    \\${${1:item}}",
      "END"
    ],
    "description": "Create a FOR loop"
  },
  "For Loop Range": {
    "prefix": "forrange",
    "body": [
      "FOR    \\${${1:index}}    IN RANGE    ${2:10}",
      "    ${3:Log}    \\${${1:index}}",
      "END"
    ],
    "description": "Create a FOR loop with range"
  },
  "If": {
    "prefix": "if",
    "body": [
      "IF    ${1:condition}",
      "    ${2:Log}    ${3:Message}",
      "END"
    ],
    "description": "Create an IF statement"
  },
  "If Else": {
    "prefix": "ifelse",
    "body": [
      "IF    ${1:condition}",
      "    ${2:Log}    ${3:True branch}",
      "ELSE",
      "    ${4:Log}    ${5:False branch}",
      "END"
    ],
    "description": "Create an IF-ELSE statement"
  },
  "If Else If": {
    "prefix": "ifelif",
    "body": [
      "IF    ${1:condition1}",
      "    ${2:Log}    ${3:First branch}",
      "ELSE IF    ${4:condition2}",
      "    ${5:Log}    ${6:Second branch}",
      "ELSE",
      "    ${7:Log}    ${8:Default branch}",
      "END"
    ],
    "description": "Create an IF-ELSE IF-ELSE statement"
  },
  "Try Except": {
    "prefix": "try",
    "body": [
      "TRY",
      "    ${1:Log}    ${2:Trying something}",
      "EXCEPT",
      "    ${3:Log}    ${4:Error occurred}",
      "FINALLY",
      "    ${5:Log}    ${6:Cleanup}",
      "END"
    ],
    "description": "Create a TRY-EXCEPT block"
  },
  "Settings Section": {
    "prefix": "settings",
    "body": [
      "*** Settings ***",
      "Documentation     ${1:Suite documentation}",
      "Library           ${2:Library}",
      "Resource          ${3:resource.resource}",
      "$0"
    ],
    "description": "Create Settings section"
  },
  "Variables Section": {
    "prefix": "variables",
    "body": [
      "*** Variables ***",
      "\\${${1:VARIABLE}}    ${2:value}",
      "@{${3:LIST}}        ${4:item1}    ${5:item2}",
      "&{${6:DICT}}        ${7:key}=${8:value}",
      "$0"
    ],
    "description": "Create Variables section"
  },
  "Test Cases Section": {
    "prefix": "testcases",
    "body": [
      "*** Test Cases ***",
      "${1:Test Name}",
      "    [Documentation]    ${2:Test documentation}",
      "    ${3:Log}    ${4:Test message}",
      "$0"
    ],
    "description": "Create Test Cases section"
  },
  "Keywords Section": {
    "prefix": "keywords",
    "body": [
      "*** Keywords ***",
      "${1:Keyword Name}",
      "    [Documentation]    ${2:Keyword documentation}",
      "    [Arguments]    ${3:\\${arg1\\}}",
      "    ${4:Log}    ${5:\\${arg1\\}}",
      "$0"
    ],
    "description": "Create Keywords section"
  },
  "Log": {
    "prefix": "log",
    "body": "Log    ${1:message}",
    "description": "Log a message"
  },
  "Should Be Equal": {
    "prefix": "shouldbe",
    "body": "Should Be Equal    ${1:first}    ${2:second}",
    "description": "Assert equality"
  },
  "Should Contain": {
    "prefix": "shouldcontain",
    "body": "Should Contain    ${1:container}    ${2:item}",
    "description": "Assert container contains item"
  },
  "Run Keyword If": {
    "prefix": "runkeywordif",
    "body": "Run Keyword If    ${1:condition}    ${2:keyword}    ${3:args}",
    "description": "Run keyword conditionally"
  },
  "Wait Until Keyword Succeeds": {
    "prefix": "waituntil",
    "body": "Wait Until Keyword Succeeds    ${1:retry}    ${2:retry_interval}    ${3:keyword}    ${4:args}",
    "description": "Wait until keyword succeeds"
  },
  "Documentation": {
    "prefix": "doc",
    "body": "[Documentation]    ${1:Documentation text}",
    "description": "Add documentation"
  },
  "Tags": {
    "prefix": "tags",
    "body": "[Tags]    ${1:tag1}    ${2:tag2}",
    "description": "Add tags"
  },
  "Setup": {
    "prefix": "setup",
    "body": "[Setup]    ${1:keyword}",
    "description": "Add setup"
  },
  "Teardown": {
    "prefix": "teardown",
    "body": "[Teardown]    ${1:keyword}",
    "description": "Add teardown"
  },
  "Arguments": {
    "prefix": "args",
    "body": "[Arguments]    ${1:\\${arg1\\}}    ${2:\\${arg2\\}}",
    "description": "Add arguments"
  },
  "Return": {
    "prefix": "return",
    "body": "[Return]    ${1:\\${result\\}}",
    "description": "Add return value"
  },
  "RETURN Statement": {
    "prefix": "RETURN",
    "body": "RETURN    ${1:\\${result\\}}",
    "description": "RETURN statement (RF 5.0+)"
  },
  "Variable": {
    "prefix": "var",
    "body": "\\${${1:variable}}",
    "description": "Insert variable"
  },
  "List Variable": {
    "prefix": "list",
    "body": "@{${1:list}}",
    "description": "Insert list variable"
  },
  "Dictionary Variable": {
    "prefix": "dict",
    "body": "&{${1:dictionary}}",
    "description": "Insert dictionary variable"
  },
  "While Loop": {
    "prefix": "while",
    "body": [
      "WHILE    ${1:condition}    limit=${2:10}",
      "    ${3:Log}    ${4:Loop iteration}",
      "END"
    ],
    "description": "Create a WHILE loop"
  },
  "For In Enumerate": {
    "prefix": "forenum",
    "body": [
      "FOR    \\${${1:index}}    \\${${2:item}}    IN ENUMERATE    @{${3:list}}",
      "    ${4:Log}    \\${${1:index}}: \\${${2:item}}",
      "END"
    ],
    "description": "FOR loop with index and item"
  },
  "For In Zip": {
    "prefix": "forzip",
    "body": [
      "FOR    \\${${1:a}}    \\${${2:b}}    IN ZIP    \\${${3:list1}}    \\${${4:list2}}",
      "    ${5:Log}    \\${${1:a}} - \\${${2:b}}",
      "END"
    ],
    "description": "FOR loop over multiple lists in parallel"
  },
  "Try Except Else Finally": {
    "prefix": "tryall",
    "body": [
      "TRY",
      "    ${1:Keyword That May Fail}",
      "EXCEPT    ${2:Error message}    AS    \\${${3:error}}",
      "    ${4:Log}    Error occurred: \\${${3:error}}",
      "ELSE",
      "    ${5:Log}    No error occurred",
      "FINALLY",
      "    ${6:Log}    Cleanup",
      "END"
    ],
    "description": "Complete TRY-EXCEPT-ELSE-FINALLY block"
  },
  "Break": {
    "prefix": "break",
    "body": "BREAK",
    "description": "Break out of a loop"
  },
  "Continue": {
    "prefix": "continue",
    "body": "CONTINUE",
    "description": "Continue to next loop iteration"
  },
  "VAR Assignment": {
    "prefix": "VAR",
    "body": "VAR    \\${${1:name}}    ${2:value}",
    "description": "VAR statement (RF 7.0+)"
  },
  "VAR List": {
    "prefix": "VARlist",
    "body": "VAR    @{${1:name}}    ${2:item1}    ${3:item2}",
    "description": "VAR list statement (RF 7.0+)"
  },
  "VAR Dict": {
    "prefix": "VARdict",
    "body": "VAR    &{${1:name}}    ${2:key1}=${3:value1}    ${4:key2}=${5:value2}",
    "description": "VAR dictionary statement (RF 7.0+)"
  },
  "Library Import": {
    "prefix": "lib",
    "body": "Library    ${1:LibraryName}",
    "description": "Import a library"
  },
  "Resource Import": {
    "prefix": "res",
    "body": "Resource    ${1:path/to/resource.resource}",
    "description": "Import a resource file"
  },
  "Suite Setup": {
    "prefix": "suitesetup",
    "body": "Suite Setup    ${1:Keyword}",
    "description": "Add suite setup"
  },
  "Suite Teardown": {
    "prefix": "suiteteardown",
    "body": "Suite Teardown    ${1:Keyword}",
    "description": "Add suite teardown"
  },
  "Test Setup": {
    "prefix": "testsetup",
    "body": "Test Setup    ${1:Keyword}",
    "description": "Add default test setup"
  },
  "Test Teardown": {
    "prefix": "testteardown",
    "body": "Test Teardown    ${1:Keyword}",
    "description": "Add default test teardown"
  },
  "Test Timeout": {
    "prefix": "timeout",
    "body": "[Timeout]    ${1:1 minute}",
    "description": "Add test/keyword timeout"
  },
  "Test Template": {
    "prefix": "template",
    "body": "[Template]    ${1:Keyword}",
    "description": "Add test template"
  },
  "Force Tags": {
    "prefix": "forcetags",
    "body": "Force Tags    ${1:tag1}    ${2:tag2}",
    "description": "Add force tags"
  },
  "Default Tags": {
    "prefix": "defaulttags",
    "body": "Default Tags    ${1:tag1}    ${2:tag2}",
    "description": "Add default tags"
  },
  "Data Driven Test": {
    "prefix": "datatest",
    "body": [
      "${1:Data Driven Test}",
      "    [Template]    ${2:Keyword Template}",
      "    ${3:arg1_value1}    ${4:arg2_value1}",
      "    ${5:arg1_value2}    ${6:arg2_value2}",
      "    ${7:arg1_value3}    ${8:arg2_value3}"
    ],
    "description": "Create a data-driven test case"
  },
  "BDD Given": {
    "prefix": "given",
    "body": "Given ${1:some precondition}",
    "description": "BDD Given statement"
  },
  "BDD When": {
    "prefix": "when",
    "body": "When ${1:some action is taken}",
    "description": "BDD When statement"
  },
  "BDD Then": {
    "prefix": "then",
    "body": "Then ${1:some result should occur}",
    "description": "BDD Then statement"
  },
  "BDD And": {
    "prefix": "and",
    "body": "And ${1:another condition}",
    "description": "BDD And statement"
  },
  "BDD But": {
    "prefix": "but",
    "body": "But ${1:an exception}",
    "description": "BDD But statement"
  },
  "Complete Robot File": {
    "prefix": "robotfile",
    "body": [
      "*** Settings ***",
      "Documentation     ${1:Suite description}",
      "Library           Collections",
      "Library           String",
      "Resource          ${2:common.resource}",
      "",
      "Suite Setup       ${3:Setup Suite}",
      "Suite Teardown    ${4:Teardown Suite}",
      "Test Setup        ${5:Setup Test}",
      "Test Teardown     ${6:Teardown Test}",
      "",
      "*** Variables ***",
      "\\${${7:VARIABLE}}    ${8:value}",
      "",
      "*** Test Cases ***",
      "${9:Test Case Name}",
      "    [Documentation]    ${10:Test description}",
      "    [Tags]    ${11:smoke}",
      "    ${12:Log}    ${13:Test message}",
      "",
      "*** Keywords ***",
      "${14:Custom Keyword}",
      "    [Documentation]    ${15:Keyword description}",
      "    [Arguments]    \\${${16:arg1}}",
      "    ${17:Log}    \\${${16:arg1}}",
      "    RETURN    \\${${18:result}}"
    ],
    "description": "Create a complete Robot Framework file structure"
  },
  "Wait Until": {
    "prefix": "waituntil",
    "body": "Wait Until Keyword Succeeds    ${1:5x}    ${2:1s}    ${3:Keyword}    ${4:args}",
    "description": "Wait until keyword succeeds"
  },
  "Run Keyword And Return Status": {
    "prefix": "rkrs",
    "body": "\\${${1:status}}=    Run Keyword And Return Status    ${2:Keyword}    ${3:args}",
    "description": "Run keyword and capture status"
  },
  "Run Keyword And Ignore Error": {
    "prefix": "rkie",
    "body": "\\${${1:status}}    \\${${2:output}}=    Run Keyword And Ignore Error    ${3:Keyword}    ${4:args}",
    "description": "Run keyword and capture error"
  },
  "Inline IF": {
    "prefix": "inlineif",
    "body": "IF    ${1:condition}    ${2:Keyword}    ${3:args}",
    "description": "Single-line IF statement"
  },
  "Skip Test": {
    "prefix": "skip",
    "body": "Skip    ${1:Reason for skipping}",
    "description": "Skip the current test"
  },
  "Skip If": {
    "prefix": "skipif",
    "body": "Skip If    ${1:condition}    ${2:Reason for skipping}",
    "description": "Conditionally skip the test"
  },
  "Embedded Arguments Keyword": {
    "prefix": "embedded",
    "body": [
      "${1:User} logs in with password ${2:password}",
      "    [Documentation]    Keyword with embedded arguments",
      "    Log    User: \\${${1:User}}",
      "    Log    Password: \\${${2:password}}"
    ],
    "description": "Create keyword with embedded arguments"
  },
  "Python Expression Variable": {
    "prefix": "pyexpr",
    "body": "\\${{ ${1:expression} }}",
    "description": "Python expression variable (RF 5.0+)"
  },
  "Evaluate Python Expression": {
    "prefix": "evalpy",
    "body": "\\${${1:result}}=    Evaluate    ${2:expression}    modules=${3:os, sys}",
    "description": "Evaluate Python expression with modules"
  },
  "Advanced Embedded Arguments Keyword": {
    "prefix": "embeddedadv",
    "body": [
      "${1:User} \"\\${${2:username}}\" ${3:performs} \"\\${${4:action}}\" ${5:on} \"\\${${6:target}}\"",
      "    [Documentation]    Keyword with multiple embedded arguments",
      "    Log    User: \\${${2:username}}",
      "    Log    Action: \\${${4:action}}",
      "    Log    Target: \\${${6:target}}"
    ],
    "description": "Create keyword with multiple embedded arguments"
  },
  "FOR Loop Over Dictionary": {
    "prefix": "fordict",
    "body": [
      "FOR    \\${${1:key}}    \\${${2:value}}    IN    &{${3:dictionary}}",
      "    Log    \\${${1:key}}: \\${${2:value}}",
      "END"
    ],
    "description": "FOR loop iterating over dictionary key-value pairs"
  },
  "Nested FOR Loops": {
    "prefix": "fornested",
    "body": [
      "FOR    \\${${1:outer}}    IN    @{${2:outer_list}}",
      "    FOR    \\${${3:inner}}    IN    @{${4:inner_list}}",
      "        Log    \\${${1:outer}} - \\${${3:inner}}",
      "    END",
      "END"
    ],
    "description": "Nested FOR loops"
  },
  "WHILE Loop with BREAK": {
    "prefix": "whilebreak",
    "body": [
      "WHILE    ${1:True}    limit=${2:100}",
      "    \\${${3:condition}}=    ${4:Check Condition}",
      "    IF    \\${${3:condition}}",
      "        BREAK",
      "    END",
      "    ${5:Do Something}",
      "END"
    ],
    "description": "WHILE loop with break condition"
  },
  "Keyword with Varargs": {
    "prefix": "kwvarargs",
    "body": [
      "${1:Keyword With Varargs}",
      "    [Documentation]    Keyword accepting variable number of arguments",
      "    [Arguments]    @{${2:args}}",
      "    FOR    \\${${3:arg}}    IN    @{${2:args}}",
      "        Log    \\${${3:arg}}",
      "    END"
    ],
    "description": "Create keyword with *args (varargs)"
  },
  "Keyword with Kwargs": {
    "prefix": "kwkwargs",
    "body": [
      "${1:Keyword With Kwargs}",
      "    [Documentation]    Keyword accepting keyword arguments",
      "    [Arguments]    &{${2:kwargs}}",
      "    FOR    \\${${3:key}}    \\${${4:value}}    IN    &{${2:kwargs}}",
      "        Log    \\${${3:key}}=\\${${4:value}}",
      "    END"
    ],
    "description": "Create keyword with **kwargs"
  },
  "Keyword with Mixed Arguments": {
    "prefix": "kwmixed",
    "body": [
      "${1:Keyword With Mixed Args}",
      "    [Documentation]    Keyword with positional, default, varargs, and kwargs",
      "    [Arguments]    \\${${2:required}}    \\${${3:optional}}=${4:default}    @{${5:varargs}}    &{${6:kwargs}}",
      "    Log    Required: \\${${2:required}}",
      "    Log    Optional: \\${${3:optional}}",
      "    Log    Varargs: @{${5:varargs}}",
      "    Log    Kwargs: &{${6:kwargs}}"
    ],
    "description": "Create keyword with all argument types"
  },
  "Library with Alias": {
    "prefix": "libalias",
    "body": "Library    ${1:SeleniumLibrary}    WITH NAME    ${2:Browser}",
    "description": "Import library with alias"
  },
  "Library with Arguments": {
    "prefix": "libargs",
    "body": "Library    ${1:DatabaseLibrary}    ${2:host}=${3:localhost}    ${4:port}=${5:5432}",
    "description": "Import library with constructor arguments"
  },
  "Remote Library": {
    "prefix": "libremote",
    "body": "Library    Remote    http://${1:localhost}:${2:8270}    WITH NAME    ${3:RemoteLib}",
    "description": "Import Remote library"
  },
  "TRY-EXCEPT with Specific Exceptions": {
    "prefix": "tryspecific",
    "body": [
      "TRY",
      "    ${1:Keyword That May Fail}",
      "EXCEPT    ${2:ValueError}:*    AS    \\${${3:error}}",
      "    Log    ValueError caught: \\${${3:error}}    level=WARN",
      "EXCEPT    ${4:TypeError}:*    AS    \\${${5:error}}",
      "    Log    TypeError caught: \\${${5:error}}    level=ERROR",
      "EXCEPT    AS    \\${${6:error}}",
      "    Log    Unknown error: \\${${6:error}}    level=ERROR",
      "END"
    ],
    "description": "TRY-EXCEPT catching specific exception types"
  },
  "TRY-EXCEPT with Glob Pattern": {
    "prefix": "tryglob",
    "body": [
      "TRY",
      "    ${1:Keyword That May Fail}",
      "EXCEPT    *${2:connection}*    type=GLOB    AS    \\${${3:error}}",
      "    Log    Connection error: \\${${3:error}}    level=WARN",
      "EXCEPT    *${4:timeout}*    type=GLOB    AS    \\${${5:error}}",
      "    Log    Timeout error: \\${${5:error}}    level=WARN",
      "END"
    ],
    "description": "TRY-EXCEPT with glob pattern matching"
  },
  "TRY-EXCEPT with Regex Pattern": {
    "prefix": "tryregex",
    "body": [
      "TRY",
      "    ${1:Keyword That May Fail}",
      "EXCEPT    ${2:.*Error.*}    type=REGEXP    AS    \\${${3:error}}",
      "    Log    Error matched: \\${${3:error}}",
      "END"
    ],
    "description": "TRY-EXCEPT with regex pattern matching"
  },
  "Set Variable If": {
    "prefix": "setvarif",
    "body": "\\${${1:result}}=    Set Variable If    ${2:condition}    ${3:true_value}    ${4:false_value}",
    "description": "Set variable conditionally"
  },
  "Create List": {
    "prefix": "createlist",
    "body": "@{${1:list}}=    Create List    ${2:item1}    ${3:item2}    ${4:item3}",
    "description": "Create a list variable"
  },
  "Create Dictionary": {
    "prefix": "createdict",
    "body": "&{${1:dict}}=    Create Dictionary    ${2:key1}=${3:value1}    ${4:key2}=${5:value2}",
    "description": "Create a dictionary variable"
  },
  "VAR with Scope": {
    "prefix": "VARscope",
    "body": "VAR    \\${${1:name}}    ${2:value}    scope=${3|LOCAL,TEST,TASK,SUITE,GLOBAL|}",
    "description": "VAR statement with scope (RF 7.0+)"
  }
}
